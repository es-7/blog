<div class="header">
	<div class="nav-header">
		<div class="navbar-logo">
			 <a href="<%- url_for() %>" class="logo"><%= config.title %></a>
			<span id="busuanzi_container_site_pv" style="color:#ccc;padding-left:20px;font-size: 14px;display:none;">
				本站总访问量100<span id="busuanzi_value_site_pv"></span>次
			</span>
			<span id="busuanzi_container_site_uv" style="color:#ccc;padding-left:20px;font-size: 14px;display:none;">
			本站访客数50<span id="busuanzi_value_site_uv"></span>人次
			</span>
		</div>
		<ul class="navbar-nav">
			<% for (var name in theme.menu){ %>
          		<li class="nav-item">
					<a class="main-nav-link" href="<%- url_for(theme.menu[name])%>"><%= __('Nav.'+name) %></a>
				</li>
			<% } %>
			<% if(config.feed && config.feed.path){ %>
				<li class="nav-item">
					<a class="main-nav-link" href="<%- url_for(config.feed.path)%>"><%= __('RSS') %></a>
				</li>
			<% } else if(theme.rss){ %>
				<li class="nav-item">
					<a class="main-nav-link" href="<%- url_for(theme.rss)%>"><%= __('RSS') %></a>
				</li>
			<% } %>
		</ul>
	</div>
</div>