<%if ( is_post() ) { %>
    <% if (post.prev || post.next){ %>
        <div class="page-nav">
            <% if (post.prev){ %>
                <a class="prev-link" href="<%- url_for(post.prev.path) %>"><% if (post.prev.title){ %> <%= post.prev.title %> <% } %></a>
            <% } %>
            <% if (post.next){ %>
                <a class="next-link" href="<%- url_for(post.next.path) %>"><% if (post.next.title){ %> <%= post.next.title %> <% } %></a>
            <% } %>
        </div>
    <% } %>
<%} else { %>
    <div class="page-nav">
        <% if(page.prev) { %>
            <a class="prev-link" href="<%- url_for(page.prev_link) %>"><%= __('Paginator.Prev') %></a>
        <% } %>

        <% if(page.next) { %>
            <a class="next-link" href="<%- url_for(page.next_link) %>"><%= __('Paginator.Next') %></a>
        <% } %>

    </div>
<% } %>